{
  "buildCommand": "pnpm install && pnpm build",
  "installCommand": "corepack enable && corepack prepare pnpm@8.15.0 --activate && pnpm install",
  "framework": "nextjs",
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    }
  }
}